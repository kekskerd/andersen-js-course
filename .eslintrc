{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "prettier/prettier": ["error"],
    "no-var": "off",
    "no-param-reassign": "off",
    "prefer-rest-params": "off",
    "object-shorthand": "off",
    "prefer-template": "off",
    "vars-on-top": "off",
    "prefer-destructuring": "off",
    "func-names": "off",
    "no-underscore-dangle": "off"
  }
}